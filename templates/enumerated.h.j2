//enumerated

typedef enum {
{% for item in items %}
    e2ap_{{name}}_{{ item.name | replace('-', '_') }} = {{ item.value }}{% if not loop.last %},{% endif %}

{% endfor %}
} e2ap_{{name}}_Root;

typedef OSUINT32 e2ap_{{name}};

/* Encode / Decode */
EXTERN int asn1PE_e2ap_{{name}} (OSCTXT* pctxt, e2ap_{{name}} value);
