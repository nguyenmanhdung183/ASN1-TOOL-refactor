EXTERN int asn1PrtToStr_e2ap_{{choices[0].dad_type_name|replace('-', '_')}}_protocolIEs_element (const char * name, e2ap_{{choices[0].dad_type_name|replace('-', '_')}}_protocolIEs_element* pvalue, char * buffer, OSSIZE bufSize);

{% else %}{#msg thường#}
typedef struct EXTERN e2ap_{{ies_name}} {
   e2ap_ProtocolIE_ID id;
