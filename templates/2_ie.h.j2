   struct{
      e2ap_{{ies_name|replace('-', '_')}}_TVALUE t;
      union {
      {% for d in choices %}
         e2ap_{{d.item_type|replace('-', '_')}} * _e2ap{{ ies_name }}_{{ d.item_type | replace('-', '_') }};
      {%endfor%}

         ASN1OpenType* extElem1;  /* extension */
      } u;
   }value;
} e2ap_{{ies_name}};

EXTERN int  asn1PE_e2ap_{{ies_name}} (OSCTXT* pctxt, e2ap_{{ies_name}}* pvalue);
