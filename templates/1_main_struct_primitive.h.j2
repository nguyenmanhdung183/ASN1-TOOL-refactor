/* {{name}}- {{metadata.primitive_name}}*/
{% if metadata.primitive_id ==  2%} {# BIT STRING A..B,...#}  
typedef struct{
    #define {{name}}_OCTET_SIZE {{(metadata.max +7)//8 }}
    UInt8 data[{{name}}_OCTET_SIZE];
} _e2ap_{{name}}_t;// BIT STRING SIZE (A..B,...) - P  ?
{% endif %}
{% if metadata.primitive_id ==  3%} {# BIT STRING N  ?#}  
typedef struct{
    UInt{{metadata.max |stdbit}} numbits;
    UInt8 data[{{ (metadata.max +7)//8 }}];
}_e2ap_{{name}}_t;// BIT STRING SIZE (N) - P  ?
{% endif %}
{% if metadata.primitive_id ==  4%} {# BIT STRING A..B  ?#}  
typedef struct{
    UInt{{metadata.max |stdbit}} numbits;
    UInt8 data[{{ (metadata.max +7)//8 }}];
}_e2ap_{{name}}_t;// BIT STRING SIZE (A..B) - P  ?
{% endif %}
{% if metadata.primitive_id == 5 %} {# INTERGER A..B,... ?#}  
typedef UInt{{metadata.max|stdbit}} _e2ap_{{name}}_t; // INTEGER (A..B,...) - P  ?
{% endif %}
{% if metadata.primitive_id ==6 %} {# INTERGER A..B#}  
typedef UInt{{metadata.max|stdbit}} _e2ap_{{name}}_t;// INTEGER (A..B) - P  ?
{% endif %}
{% if metadata.primitive_id ==  8%} {# OCTET STRING SIZE N ? #}  
typedef struct{
    #define {{name}}_MAX_BYTES {{size}}
    UInt8 {{name}}[{{name}}_MAX_BYTES];
}_e2ap_{{name}}_t; //OCTET STRING SIZE N - P  ?
{% endif %}
{% if metadata.primitive_id ==  9%} {# OCTET STRING ?#} 
typedef struct{
    UInt{{metadata.max |stdbit}} num_string_len; /// dungnm23 fix size
    UInt8 * string_data;
}_e2ap_{{name}}_t; //OCTET STRING - P  ?
{% endif %}
{% if metadata.primitive_id == 10 %} {# Printable String A..B,...  ?#} 
typedef const char* _e2ap_{{name}}_t;//Printable STRING SIZE (A..B,...) - P  ?
{% endif %} 
{% if metadata.primitive_id ==  13%} {# ENUMERATED  ?#} 
typedef enum{
    {% for enum in items %}    
    E2AP_{{enum.name|camel_to_upper_snake | replace("-", "_")}} = {{enum.value}}{% if not loop.last %},{% endif %}
    {% endfor %}  

}_e2ap_{{name}}_et;// ENUMERATED - P  ?
{% endif %}