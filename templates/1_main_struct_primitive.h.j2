{% if metadata.primitive_id ==  2%} {# BIT STRING A..B,...#}  
typedef struct{
    Uint{{metadata.max|stdbit}} numbits;
    UInt8* data;
} _e2ap_{{name}}_t;// BIT STRING SIZE (A..B,...) - P
{% endif %}
{% if metadata.primitive_id ==  3%} {# BIT STRING N#}  
typedef struct{
    unsigned int numbits;
    unsigned char data[{{ (metadata.max +7)//8 }}];
}_e2ap_{{name}}_t;// BIT STRING SIZE (N) - P
{% endif %}
{% if metadata.primitive_id ==  4%} {# BIT STRING A..B#}  
typedef struct{
    unsigned int numbits;
    unsigned char data[{{ (metadata.max +7)//8 }}];
}_e2ap_{{name}}_t;// BIT STRING SIZE (A..B) - P
{% endif %}
