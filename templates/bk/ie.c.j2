void asn1Free_e2ap_{{ies_name}} (OSCTXT* pctxt, e2ap_{{ies_name}}* pvalue)
{
   if(0==pvalue) return;
   switch(pvalue->value.t){
   {% for d in choices %}
      case T_E2AP_PDU_Contents_e2ap_{{ ies_name }}_{{ d.item_type | replace('-', '_') }}:
         asn1Free_e2ap_{{ d.item_type | replace('-', '_') }} (pctxt, pvalue->value.u._e2ap{{ ies_name }}_{{ d.item_type | replace('-', '_')}});
         rtxMemFreePtr(pctxt, (void*)pvalue->value.u._e2ap{{ ies_name }}_{{ d.item_type | replace('-', '_')}});
         pvalue->value.u._e2ap{{ ies_name }}_{{ d.item_type | replace('-', '_')}} = 0;
         break;
   {% endfor %}
      case T_E2AP_PDU_Contents_e2ap_{{ ies_name }}_UNDEF_:
         if(0!=pvalue->value.u.extElem1){
             rtxMemFreePtr(pctxt, pvalue->value.u.extElem1->data);
             rtxMemFreePtr(pctxt, pvalue->value.u.extElem1);
             pvalue->value.u.extElem1 =0;
         }
         break;
         default:;
   }
}

