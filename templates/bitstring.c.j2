//bitstring (n) mau tu NID xn
{% if metadata.primitive_id == 3 %}

EXTERN int asn1PE_e2ap_{{name}}(OSCTXT* pctxt, e2ap_{{name}}* pvalue){
    int stat = 0;
    RTXCTXTPUSHTYPENAME (pctxt, "{{name|replace('_','-')}}");
    PU_SETSIZECONSTRAINT(pctxt, OSUINTCONST({{metadata.fix_size}}), OSUINTCONST({{metadata.fix_size}}), 0, 0);
    stat = pe_BitString(pctxt, OS_MIN(pvalue->numbits, metadata.fix_size), pvalue->data);
    if (stat != 0) return LOG_RTERR (pctxt, stat);
    RTXCTXTPOPTYPENAME (pctxt);
    return stat;
}

EXTERN int asn1PD_e2ap_{{name}}(OSCTXT* pctxt, e2ap_{{name}}* pvalue){
    int stat = 0;
    RTXCTXTPUSHTYPENAME (pctxt, "{{name|replace('_','-')}}");
    PU_SETSIZECONSTRAINT(pctxt, OSUINTCONST({{metadata.fix_size}}), OSUINTCONST({{metadata.fix_size}}), 0, 0);
    stat = pd_BitString(pctxt, &pvalue->numbits, pvalue->data, sizeof(pvalue->data));
    if (stat != 0) return LOG_RTERR (pctxt, stat);
    RTXCTXTPOPTYPENAME (pctxt);
    return stat;
}

int asn1Init_e2ap_{{name}}(e2ap_{{name}}* pvalue){
    if (pvalue == 0) return RTERR_NULLPTR;
    pvalue->numbits = 0;
    return 0;
}
{% endif %}


