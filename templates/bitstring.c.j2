EXTERN int asn1Init_e2ap_{{name}}(e2ap_{{name}}* pvalue){
    if (pvalue == 0) return RTERR_NULLPTR;
    pvalue->numbits = 0;
    pvalue->data = 0;
    return 0;
}
EXTERN void asn1Free_e2ap_{{name}}(OSCTXT* pctxt, e2ap_{{name}}* pvalue){
    if (pvalue == 0) return;
    if(pvalue->numbits>0){
        rtxMemFreePtr(pctxt, (void*)pvalue->data);
        pvalue->data = 0;
        pvalue->numbits = 0;
    }
}

